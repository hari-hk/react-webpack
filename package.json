{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "React webpack starter kit",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist/**",
    "start": "webpack serve ./index.js --mode development --hot --open",
    "build": "npm run prebuild & webpack ./index.js --mode production",
    "lint": "eslint . --ignore-pattern .eslintignore",
    "lint:fix": "npm run lint & --fix ",
    "commit": "git cz",
    "generate": "plop"
  },
  "keywords": [
    "react",
    "react-webpack",
    "react-boilerplate"
  ],
  "author": "Hari hara kumar",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "scss": "^0.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "git-cz": "^4.7.6",
    "html-webpack-plugin": "^5.3.1",
    "plop": "^2.7.4",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
